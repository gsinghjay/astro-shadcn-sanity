{
  "name": "ywcc-capstone-template",
  "version": "1.9.0",
  "description": "YWCC Capstone Websites â€” Astro + Sanity monorepo",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev --workspace=astro-app\" \"npm run dev --workspace=studio\"",
    "dev:storybook": "concurrently --kill-others \"npm run dev --workspace=astro-app\" \"npm run dev --workspace=studio\" \"npm run storybook --workspace=astro-app\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "npm run test:unit --workspace=astro-app",
    "test:e2e": "npx playwright test",
    "test:chromium": "npx playwright test --project=chromium",
    "test:headed": "npx playwright test --headed --project=chromium",
    "test:ui": "npx playwright test --ui",
    "test:unit:watch": "npm run test:unit:watch --workspace=astro-app",
    "test:unit:coverage": "npm run test:unit:coverage --workspace=astro-app",
    "storybook": "npm run storybook --workspace=astro-app",
    "typegen": "npm run typegen -w studio"
  },
  "keywords": [
    "Sanity",
    "Astro",
    "Headless CMS",
    "Visual Editing",
    "Live Content API (LCAPI)",
    "Real-time"
  ],
  "bugs": {
    "url": "https://github.com/gsinghjay/astro-shadcn-sanity/issues"
  },
  "homepage": "https://github.com/gsinghjay/astro-shadcn-sanity#readme",
  "workspaces": [
    "studio",
    "astro-app"
  ],
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.58.2",
    "@seontechnologies/playwright-utils": "^3.13.1",
    "chromatic": "^15.1.1",
    "concurrently": "^9.1.0"
  },
  "type": "module"
}
