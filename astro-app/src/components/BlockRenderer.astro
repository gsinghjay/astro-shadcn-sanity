const { blocks } = Astro.props;
---

{blocks.map((block) => {
  // Custom blocks (block prop pattern)
  switch (block._type) {
    case 'heroBanner':
      return <HeroBanner block={block} />;
    case 'statsRow':
      return <StatsRow block={block} />;
    case 'featureGrid':
      return <FeatureGrid block={block} />;
    case 'faqSection':
      return <FaqSection block={block} />;
    case 'ctaBanner':
      return <CtaBanner block={block} />;
    case 'sponsorCards':
      return <SponsorCards block={block} />;
    case 'timeline':
      return <Timeline block={block} />;
    case 'teamGrid':
      return <TeamGrid block={block} />;
    case 'logoCloud':
      return <LogoCloud block={block} />;
    case 'textWithImage':
      return <TextWithImage block={block} />;
    case 'contactForm':
      return <ContactForm block={block} />;
    case 'richText':
      return <RichText block={block} />;
    default: {
      // fulldotdev/ui blocks (spread props pattern)
      const FdComponent = fulldotdevBlocks[(block as any)._type]
      return FdComponent ? <FdComponent {...(block as any)} /> : null
    }
  }
})}
